{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "source": {
      "include": ["packages"],
      "exclude": ["node_modules", "packages/**/node_modules"],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
      "template": "node_modules/docdash",
      "encoding": "utf8",
      "destination": "./public/jsdoc-out/",
      "recurse": true
  }
}